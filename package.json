    "autoprefixer": "^10.4.18",
    "gatsby": "^5.13.3",
    "gatsby-plugin-canonical-urls": "^5.13.1",
    "gatsby-plugin-eslint": "^4.0.4",
    "gatsby-plugin-html-attributes": "^1.0.5",
    "gatsby-plugin-image": "^3",
    "gatsby-plugin-manifest": "^5.13.1",
